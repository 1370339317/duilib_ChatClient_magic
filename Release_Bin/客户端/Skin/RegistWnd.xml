        </HorizontalLayout>

        <HorizontalLayout inset="40,10, 30,0" height="45">
          <Label text="密码：" textcolor="#FF000000" font="0" width="80" align="right"/>
          <Edit name="Password" height="35" width="200" inset="5,5, 1,1" password="true"  bkcolor="#FFFFFFFF" bordersize="1,1,1,1" bordercolor="#FF555555" font="0"/>
        </HorizontalLayout>

        <HorizontalLayout inset="40,10, 30,0" height="45">
          <Label text="确认密码：" textcolor="#FF000000" font="0" width="80" align="right"/>
          <Edit name="ComfigPassword" height="35" width="200" inset="5,5, 1,1" password="true"  bkcolor="#FFFFFFFF" bordersize="1,1,1,1" bordercolor="#FF555555" font="0"/>
        </HorizontalLayout>

        <HorizontalLayout inset="40,10, 30,0" height="45">
          <Label text="昵称：" textcolor="#FF000000" font="0" width="80" align="right"/>
          <Edit name="NickName" height="35" width="200" inset="5,5, 1,1" bkcolor="#FFFFFFFF" bordersize="1,1,1,1" bordercolor="#FF555555" font="0"/>
        </HorizontalLayout>

        <HorizontalLayout inset="40,10, 30,0" height="45">
          <Label text="个性签名：" textcolor="#FF000000" font="0" width="80" align="right"/>
